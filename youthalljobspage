<script src="/bundles/app/js/lazyload.js?v370"></script>
    <script src="/assets/js/app.min.js?v370"></script>

    <script src="https://unpkg.com/multiple-select@1.3.1/dist/multiple-select.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#filter").on("change", ["input:checkbox", "select"], function () {
                $("#filter").submit();
            });
            $('select[multiple="multiple"]').multipleSelect({
                filter: true,
                selectAll: false,
            });
        });
    </script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
    <script src="/bundles/app/js/applications.js?v370"></script>
    <script>
        $(document).ready(function() {
            let notificationsList = $('#userNotifications');
            let newNotificationAlert = $('.site-nav_user-notification-new');
            let listItems = "";
            $.ajax({
                url: '/en/ajax/get/notifications',
                method: 'GET',
                success: function(response) {
                    notificationsList.find('.notifications-loading').hide();

                    if (response.success){
                        $.each(response.data['notifications'], function(index, notification) {
                            listItems +=
                                `<li id="notificationItem" data-key="${index}">
                                    <a href="${notification.url}" class="notification-link">
                                        <div class="notification" direction="row" wrap="nowrap">
                                            <div class="notification-icon" order="1">
                                                 <div class="notification-icon-avatar">
                                                    <span>
                                                        <i class="fa fa-bell-o" aria-hidden="true"></i>
                                                    </span>
                                                 </div>
                                            </div>
                                             <div class="notification-content" order="2">
                                                <div class="notification-content-box">
                                                    <div>
                                                        <span class="notification-content-box-tag">İlanlar</span>
                                                        <div class="notification-content-box-title">${notification.title}</div>
                                                        <p class="notification-content-box-description">${notification.message}</p>
                                                    </div>
                                                </div>
                                             </div>
                                             <div class="notification-date" order="3">
                                                <div class="notification-date-description">
                                                    <small>${notification.createdAt}</small>
                                                </div>
                                             </div>
                                        </div>
                                    </a>
                                    <div class="notification-nav">
                                       <div class="dropdown">
                                          <a href="#" class="dropdown-toggle" role="button" id="notificationButton" data-toggle="dropdown">
                                            <i class="fa fa-ellipsis-h c-dashboard-icon-dd" aria-hidden="true"></i>
                                          </a>
                                          <ul class="dropdown-menu" style="width:200px; padding:5px;">
                                            <li style="padding: 5px 0; margin: 0">
                                                <a class="dropdown-item mark-as-seen" data-notification-id="${notification.id}">Okundu Olarak İşaretle</a>
                                            </li>
                                            <!-- <li style="padding: 5px 0; margin: 0">
                                                <a class="dropdown-item" id="deleteNotification" data-notification-id="${notification.id}" data-key="${index}">Sil</a>
                                            </li> -->
                                          </ul>
                                       </div>
                                    </div>
                                </li>`
                        });

                        notificationsList.append(listItems);

                        // nested dropdown
                        $('.dropdown-menu a.dropdown-toggle').on('click', function() {
                            if (!$(this).next().hasClass('show')) {
                                $(this).parents('.dropdown-menu').first().find('.show').removeClass('show');
                            }
                            let $subMenu = $(this).next('.dropdown-menu');
                            $subMenu.toggleClass('show');

                            $(this).parents('li.nav-item.dropdown.show').on('hidden.bs.dropdown', function(e) {
                                $('.dropdown-submenu .show').removeClass('show');
                            });

                            return false;
                        });

                        // notification mark seen
                        $('.mark-as-seen').on('click', function () {
                            let notificationId = $(this).data('notification-id');
                            $.ajax({
                                url: '/en/ajax/mark/notification',
                                method: 'POST',
                                data: {
                                    id: notificationId
                                },
                                success: function (res) {
                                    if (res.status === 'success' && res['hideNotificationIndicator']){
                                        newNotificationAlert.hide();
                                    }
                                }
                            })
                        })

                        // remove element and delete notification
                        $('a[id="deleteNotification"]').each(function () {
                            $(this).on('click', function () {
                                let notificationId = $(this).data('notification-id');
                                let notificationItem = $(this).data('key');
                                $.ajax({
                                    url: '/en/ajax/delete/notification',
                                    method: 'POST',
                                    data: {
                                        id: notificationId
                                    },
                                    success: function (res) {
                                        if (res.success){
                                            $(`li[data-key="${notificationItem}"]`).remove()
                                            if (res['newNotificationIndicatorHide']){
                                            newNotificationAlert.hide();
                                            }
                                        }
                                    }
                                })
                            })
                        })

                    }else{
                        listItems = `<li style="display: flex; align-items: center; justify-content: center; flex-direction: column;">
<img src="/assets/img/ui/no-new-notification.png?v370" style="width: 50%; background: #f1f1f1; padding: 40px; border-radius: 50%; overflow: visible;" />
<p style="font-size: 23px; font-weight: bold; margin-top: 20px;">Yeni bir bildirimin yok.</p>
</li>`;
                        notificationsList.append(listItems);
                        notificationsList.css({
                            'display': 'flex',
                            'align-items': 'center',
                            'justify-content': 'center',
                            'flex-direction': 'column',
                            'height': '90%',
                        });
                    }

                    if (!response.data['all_seen']){
                        newNotificationAlert.show()
                    }
                }
            });
        });
    </script>
    </body>
</html>
